  pod 'OpenCV-Dynamic', :podspec => "https://raw.githubusercontent.com/hulop/blelocpp/v1.3.6/platform/ios/podspecs/OpenCV-Dynamic.podspec"
  pod 'picojson', :podspec => './podspecs/picojson.podspec'
end

